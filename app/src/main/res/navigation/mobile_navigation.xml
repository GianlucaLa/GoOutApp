<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_showGroup">

    <fragment
        android:id="@+id/nav_showGroup"
        android:name="it.gooutapp.showGroup.ShowGroupFragment"
        android:label="@string/menu_show_groups"
        tools:layout="@layout/fragment_show_group" />

    <fragment
        android:id="@+id/nav_createGroup"
        android:name="it.gooutapp.home.HomeFragment"
